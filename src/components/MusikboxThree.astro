---
const { songThree } = Astro.props;
---

<iframe
  src={`https://open.spotify.com/embed/track/${songThree}?utm_source=generator&theme=0`}
  frameborder="0"
  allowfullscreen=""
  allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"
  loading="lazy"
>
</iframe>

<style>
  iframe {
    border-radius: 12px;
    width: 90%;
    height: 100px;
  }
</style>
